{
  "manifestVersion": "1.0",
  "id": "app-name",
  "name": "App Name",
  "version": "1.0.0",
  "description": "A description of the app",
  "developer": {
    "name": "Publisher Name",
    "websiteUrl": "https://example.com/"
  },
  "documentation": "http://example.com/docs",
  "license": "Apache-2.0",
  "icons": {
    "outline": "/path/to/outline/icon.png",
    "color": "/path/to/color/icon.png"
  },
  "baseUrl": "https://example.com",
  "permissions": {
    "scopes": []
  },
  "modules": {
    "projectPages": [
      {
        "name": "My Project Page",
        "url": "/index.html",
        "internalRoute": "MyProjectPage",
        "icon": {
          "url": "/path/to/icon.png", // or "svg"
          "title": "My Project Page" // or use "name" instead
        }
      }
    ]
  }
}
